{
  "name": "@slub/edb-graph-traversal_src",
  "version": "1.0.0",
  "type": "module",
  "description": "A TypeScript library for traversing RDF Graphs to extract and query data matching JSON Schemas, facilitating the development of semantic user interfaces.",
  "scripts": {
    "build:package.json": "make-package-json",
    "build": "tsup && pnpm run build:package.json",
    "lint": "eslint \"**/*.ts*\"",
    "lint-fix": "eslint --fix \"**/*.ts*\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch --color",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "docs": "typedoc --out docs"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@rdfjs/dataset": "^2.0.1",
    "@rdfjs/parser-n3": "^2.0.1",
    "@slub/edb-build-helper": "workspace:*",
    "@slub/edb-tsconfig": "workspace:*",
    "@slub/edb-tsup-config": "workspace:*",
    "@types/json-schema": "^7.0.14",
    "@types/lodash": "^4.14.200",
    "@types/rdfjs__data-model": "^2.0.7",
    "eslint-config-edb": "workspace:*",
    "rdf-dataset-ext": "^1.0.1"
  },
  "dependencies": {
    "@jsonforms/core": "3.1.0",
    "@rdfjs/data-model": "^1.3.4",
    "@rdfjs/namespace": "^2.0.0",
    "@rdfjs/types": "^1.1.0",
    "@slub/edb-core-utils": "workspace:*",
    "@slub/json-schema-utils": "workspace:*",
    "@tpluscode/rdf-ns-builders": "^2.0.1",
    "add": "^2.0.6",
    "clownface": "^1.5.1",
    "lodash": "^4.17.21",
    "pnpm": "^8.15.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slub/exhibition-live.git"
  },
  "keywords": [
    "RDF",
    "JSON Schema",
    "Semantic Web",
    "Data Extraction",
    "Graph Traversal"
  ],
  "license": "MIT"
}
